{
    "name": "vee-type-safe",
    "version": "3.1.2",
    "description": "Simple TypeScript type checking utility library.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "keywords": [
        "utility",
        "utils",
        "typescript",
        "typechecks",
        "type conformance",
        "duck typing"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Veetaha/vee-type-safe.git"
    },
    "scripts": {
        "docs":  "./node_modules/.bin/typedoc --options typedoc.json index.ts",
        "test":  "mocha build/test/tests.js",
        "build": "tsc",
        "build-test": "npm run build && npm run test",
        

        "bootstrap":  "npm i && npm build",
        "pre-deploy": "npm run build && npm run test && npm run docs"
    },
    "author": "Vitaliy Kryvenko",
    "license": "MIT",
    "dependencies": {
        "@types/express": "latest",
        "@types/node": "latest",
        "http-status-codes": "latest",
        "is-iso-date": "latest",
        "typescript": "3.3.1"
    },
    "devDependencies": {
        "@types/chai": "latest",
        "@types/mocha": "latest",
        "chai": "latest",
        "mocha": "latest",
        "tslint": "^5.11.0",
        "typedoc": "^0.13.0"
    }
}
